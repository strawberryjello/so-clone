%h2 Questions

= link_to 'Ask Question', new_question_path
- if session[:user]
  / why doesn't session[:user][:login] work?
  = session[:user]['login']
  = link_to 'Logout', users_logout_path
- else
  = link_to 'Sign Up', new_user_path
  = link_to 'Login', users_login_path

%hr

%table
  - @questions.each do |question|
    %tr
      %td
        / alternatively: link_to question_path question do
        = link_to question do
          = question.title
